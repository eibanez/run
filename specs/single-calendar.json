{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json", 
  "data": {
    "format": {
      "parse": {
        "date": "date:'%Y-%m-%d'"
      }
    }, 
    "name": "data", 
    "url": "https://raw.githubusercontent.com/eibanez/run/master/data.json"
  }, 
  "encoding": {
    "color": {
      "field": "type", 
      "legend": {
        "title": "Type"
      }, 
      "scale": {
        "range": [
          "#4BBA3E", 
          "#528ECD", 
          "#CD6752"
        ]
      }, 
      "sort": [], 
      "type": "nominal"
    }, 
    "row": {
      "field": "date", 
      "header": {
        "labelOrient": "right", 
        "title": null
      }, 
      "timeUnit": "year", 
      "type": "temporal"
    }, 
    "size": {
      "field": "distance", 
      "legend": {
        "title": "Distance (miles)"
      }, 
      "type": "quantitative"
    }, 
    "x": {
      "axis": {
        "title": "Week"
      }, 
      "field": "date", 
      "timeUnit": "week", 
      "type": "temporal"
    }, 
    "y": {
      "axis": {
        "title": null
      }, 
      "field": "date", 
      "timeUnit": "day", 
      "type": "ordinal"
    }
  }, 
  "height": 150, 
  "mark": {
    "tooltip": {
      "content": "data"
    }, 
    "type": "point"
  }, 
  "spacing": 5, 
  "width": 450
}