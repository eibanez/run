{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "data": {
    "format": {
      "parse": {
        "date": "utc:'%Y-%m-%dT%H:%M:%S'"
      }
    },
    "name": "data",
    "url": "https://raw.githubusercontent.com/eibanez/run/master/data.json"
  },
  "encoding": {
    "x": {
      "axis": {
        "title": null
      },
      "field": "date",
      "timeUnit": "yearmonth",
      "type": "temporal"
    },
    "y": {
      "aggregate": "sum",
      "axis": {
        "title": "Distance (miles)"
      },
      "field": "distance",
      "type": "quantitative"
    }
  },
  "height": 250,
  "mark": {
    "tooltip": {
      "content": "data"
    },
    "type": "bar"
  },
  "width": 500
}