{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json", 
  "data": {
    "format": {
      "parse": {
        "date": "date:'%Y-%m-%d'"
      }
    }, 
    "name": "data", 
    "url": "https://raw.githubusercontent.com/eibanez/run/master/data.json"
  }, 
  "encoding": {
    "color": {
      "field": "date", 
      "legend": {
        "title": null
      }, 
      "timeUnit": "year", 
      "type": "nominal"
    }, 
    "column": {
      "field": "date", 
      "header": {
        "title": null
      }, 
      "timeUnit": "month", 
      "type": "temporal"
    }, 
    "x": {
      "axis": null, 
      "field": "date", 
      "timeUnit": "year", 
      "type": "nominal"
    }, 
    "y": {
      "aggregate": "sum", 
      "axis": {
        "title": "Distance (miles)"
      }, 
      "field": "distance", 
      "type": "quantitative"
    }
  }, 
  "height": 250, 
  "mark": {
    "tooltip": {
      "content": "data"
    }, 
    "type": "bar"
  }, 
  "spacing": 2, 
  "width": 40
}