{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json", 
  "data": {
    "format": {
      "parse": {
        "date": "date:'%Y-%m-%d'"
      }
    }, 
    "name": "data", 
    "url": "https://raw.githubusercontent.com/eibanez/run/master/data.json"
  }, 
  "encoding": {
    "color": {
      "field": "type", 
      "legend": {
        "title": null
      }, 
      "scale": {
        "range": [
          "#4BBA3E", 
          "#528ECD", 
          "#CD6752"
        ]
      }, 
      "sort": [], 
      "type": "nominal"
    }, 
    "tooltip": [
      {
        "field": "title", 
        "title": "Title"
      }, 
      {
        "field": "name", 
        "title": "Name"
      }, 
      {
        "field": "type", 
        "title": "Type"
      }, 
      {
        "field": "date", 
        "timeUnit": "yearmonthday", 
        "title": "Date"
      }, 
      {
        "field": "distance", 
        "format": ".1f", 
        "title": "Distance"
      }, 
      {
        "field": "duration", 
        "title": "Duration"
      }, 
      {
        "field": "pace", 
        "format": ".2f", 
        "title": "Pace"
      }, 
      {
        "field": "source", 
        "title": "Source"
      }
    ], 
    "x": {
      "axis": {
        "title": null
      }, 
      "field": "date", 
      "type": "temporal"
    }, 
    "y": {
      "axis": {
        "title": "Pace (min/mile)"
      }, 
      "field": "pace", 
      "scale": {
        "zero": false
      }, 
      "type": "quantitative"
    }
  }, 
  "height": 250, 
  "mark": "point", 
  "selection": {
    "grid": {
      "bind": "scales", 
      "type": "interval"
    }
  }, 
  "spacing": 5, 
  "width": 450
}