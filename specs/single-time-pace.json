{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "data": {"url": "https://raw.githubusercontent.com/eibanez/run-test/master/data.json"},
  "selection": {
    "grid": {
      "type": "interval", "bind": "scales"
    }
  },
  "height": 250,
  "width": 450,
  "spacing": 5,
  "mark": {"type": "point", "tooltip": true},
  "encoding": {
    "x": {
      "field": "start",
      "type": "temporal",
      "axis": {"title": null}
    },
    "y": {
      "field": "pace",
      "type": "quantitative",
      "axis": {"title": "Avg. speed (min/miles)"},
      "scale": {"domain": [7, 12.25]}
    },
    "color": {
      "field": "type",
      "type": "nominal",
      "sort": [],
      "legend": {"title": null}
    }
  }
}
